<html lang="en">
<body>

<% doctors.forEach(function (doctor) => { %>
    for(let i = 0; i < doctors.length; i++)  { %>
    <div class="doctor <%- doctors[i].specialization %> lg:w-1/4 md:w-1/2 p-4 w-full">
        <a class="block relative h-48 rounded overflow-hidden">
            <img alt="ecommerce" class="object-cover object-center w-full h-full block"
                 src="https://thumbs.dreamstime.com/b/doctor-icon-vector-stethoscope-medical-consultation-physician-profile-symbol-male-avatar-glyph-pictogram-doctor-icon-104463219.jpg"/>
        </a>
        <div class="mt-4">
            <h3 class="text-gray-500 text-xs tracking-widest title-font mb-1"><%= doctors[i].specialization %></h3>
            <h2 class="text-gray-900 title-font text-lg font-medium"><%= doctors[i].name %></h2>
            <p class="mt-1"><%= doctors[i].hospital %></p>
            <p class="mt-1"><%= doctors[i].experience; %></p>
            <a href="#" class="text-indigo-500 inline-flex items-center md:mb-2 lg:mb-0">BOOK</a>
        </div>
    </div>
<% })} %>
<script>

</script>
</body>
</html>